    "semver": "^7.3.2"
  },
  "devDependencies": {
    "@tsconfig/node12": "^1.0.0",
    "@types/semver": "^7.2.0",
    "@zeit/ncc": "^0.22.1",
    "jest": "^26.0.1",
    "js-yaml": "^3.13.1",
    "nock": "^12.0.3",
    "standard": "^14.3.4",
    "typescript": "^3.9.2"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/tests/setup.js"
    ]
